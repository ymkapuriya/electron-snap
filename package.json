{
  "name": "ymk-electron-snap",
  "version": "1.0.3",
  "description": "An Electron Snap",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build",
    "dist": "electron-builder --linux snap",
    "test": "node test.js",
    "release": "electron-builder --publish always"
  },
  "repository": "https://github.com/ymkapuriya/electron-snap.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "snap"
  ],
  "author": "Yogesh Kapuriya",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {},
  "build": {
    "appId": "com.example.app",
    "productName": "ymk-electron-snap",
    "linux": {
      "target": "dir",
      "publish": [
        "github"
      ]
    },
    "snap": {
      "plugs": [
        "desktop",
        "desktop-legacy",
        "home",
        "wayland",
        "unity7",
        "browser-support",
        "network",
        "gsettings",
        "audio-playback",
        "pulseaudio",
        "opengl"
      ]
    }
  },
  "publish": {
    "provider": "github",
    "repo": "https://github.com/ymkapuriya/electron-snap.git",
    "owner": "Yogesh Kapuriya",
    "vPrefixedTagName": true,
    "private": true,
    "channel": "latest",
    "releaseType": "draft"
  }
}